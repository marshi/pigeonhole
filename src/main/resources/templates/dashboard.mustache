<!doctype html>
<html lang="ja">
<head>
  <meta charset="UTF-8">
  <title>ダッシュボード</title>
  <link rel="stylesheet" href="/css/sanitize.css">
  <link rel="stylesheet" href="/css/container.css">
  <link rel="stylesheet" href="/css/content.css">
  <link rel="stylesheet" href="/css/header.css">
  <link rel="stylesheet" href="/css/dashboard.css">
</head>
<body>
<div class="container header">
  <span class="header__servicename">Pigeonhole</span>
</div>
<div class="container content"><!--main-->
  <div class="dashboard">
    <table>
      <tr class="dashboard__row">
        <th class="dashboard__cell">ホスト</th>
        <th class="dashboard__cell">ブランチ</th>
        <th class="dashboard__cell">デプロイ日時</th>
      </tr>
      {{#.}}
        <tr class="dashboard__row">
          <td class="dashboard__cell">{{hostMachineName}}</td>
          <td class="dashboard__cell">{{branchName}}</td>
          <td class="dashboard__cell">{{deployTime}}</td>
        </tr>
      {{/.}}
    </table>
  </div>
</div>
</body>
</html>